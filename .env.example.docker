NODE_ENV=production

NODE_PORT=3001
NODE_HOST=0.0.0.0

HOST_ADDRESS=http://localhost:3001

# Host of the database
DB_HOST=pg
TEST_DB_HOST=pg_test

# Port of the db server, default is 5432 for psql
DB_PORT=5432
TEST_DB_PORT=5433

# Name of the database
DB_NAME=substrate
TEST_DB_NAME=substrate

# The db we are using
DB_DIALECT=postgres
# Password of the user which owns the database
DB_PASSWORD=admin
# User which owns the database
DB_USER=postgres

COOKIE_SECRET=substratestartkitapp
COOKIE_DOMAIN=localhost
COOKIE_HTTP_ONLY=false
COOKIE_SECURE=false
COOKIE_NAME=substratestartkitapp.sid

SESSION_TABLE=session

GITHUB_APP_CLIENT_ID=
