{
  "name": "substrate-startkit-gui-api",
  "version": "1.0.0",
  "description": "API for substrate startkit gui frontend",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:integration": "jest int",
    "test:unit": "jest unit",
    "start": "npm run serve",
    "dev": "nodemon",
    "build": "npm run build-ts",
    "windows:build": "npm run windows:build-ts",
    "serve": "node dist/src/",
    "build-ts": "rm -rf dist/ && tsc",
    "windows:build-ts": "rd /s /q \"dist\" && tsc",
    "migrate": "npm run build && sequelize db:migrate",
    "seed": "npm run build && sequelize db:seed:all",
    "windows:migrate": "npm run windows:build && sequelize db:migrate",
    "windows:seed": "npm run windows:build && sequelize db:seed:all",
    "create:migration": "sequelize migration:generate --name",
    "create:seed": "sequelize seed:generate --name",
    "undo:migrations": "yarn build && sequelize db:migrate:undo:all",
    "windows:undo:migrations": "yarn windows:build && sequelize db:migrate:undo:all",
    "undo:seed": "yarn build && sequelize db:seed:undo:all",
    "windows:undo:seed": "yarn windows:build && sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MVPWorkshop/substrate-startkit-gui-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MVPWorkshop/substrate-startkit-gui-api/issues"
  },
  "homepage": "https://github.com/MVPWorkshop/substrate-startkit-gui-api#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "connect-session-sequelize": "^7.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.27.0",
    "ncp": "^2.0.0",
    "nodegit": "^0.27.0",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "ps-tree": "^1.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.13",
    "sequelize-typescript": "^1.1.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^26.0.10",
    "@types/mkdirp": "^1.0.1",
    "@types/ncp": "^2.0.4",
    "@types/node": "^14.0.13",
    "@types/nodegit": "^0.26.7",
    "@types/passport": "^1.0.4",
    "@types/passport-github2": "^1.2.4",
    "@types/ps-tree": "^1.1.0",
    "@types/uuid": "^8.0.1",
    "@types/validator": "^13.0.0",
    "jest": "^26.4.1",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
